<script type="text/javascript">
function shownextgalleryitem(el, src)
{
	var list = getlistofgalleryitems();
	var nextsrc = gallerygetnextimage(src,list);
	$(el).css("background-image","url('"+nextsrc+"')").attr("onclick",'return shownextgalleryitem(this,\''+nextsrc+'\');');
	
	return false;
}
function removegallerycontainer(el)
{
	$("#gimagepopup").remove();
	return false;
}
function getKeyByValue(object, value) {
  return Object.keys(object).find(key => object[key] === value);
}
function getlistofgalleryitems()
{
	var galleryimages = [];
	$('.img-responsivebig').each(function( index, value ) {
	  galleryimages[index] = $( this ).attr('src');
	});
	return galleryimages;
}

function gallerygetnextimage(src, galleryimages)
{
	var currentkey = getKeyByValue(galleryimages,src);
	var nextkey = parseInt(currentkey)+1;
	if (galleryimages[nextkey] == null){
		nextkey = 0;
	}
	return galleryimages[nextkey];
}
$(function() {
var galleryimages = [];
$('.img-responsivebig').each(function( index, value ) {
  galleryimages[index] = $( this ).attr('src');
});
function gallerygetnextimage(src, galleryimages)
{
	var currentkey = getKeyByValue(galleryimages,src);
	var nextkey = parseInt(currentkey)+1;
	if (galleryimages[nextkey] == null){
		nextkey = 0;
	}
	return galleryimages[nextkey];
}
 $('.img-responsivebig').on('click tap',function(){
	var el = $(this);
	var imglink = el.attr('src');
	var nextimage = gallerygetnextimage(imglink,galleryimages);
	var newel = '<div class="imagepopupc noselect" id="gimagepopup"><div class="imagepopup" style="background-image:url(\''+imglink+'\')" onclick="return shownextgalleryitem(this,\''+imglink+'\');"><a href="#" onclick="return removegallerycontainer()"> X </a></div></div>';
	$('body').prepend(newel);
 });
})
</script>
